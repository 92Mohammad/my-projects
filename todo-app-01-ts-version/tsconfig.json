{
  "compilerOptions": {
    "target": "es2016",
     "lib": [
       "es2016", "dom"
     ],
     "jsx": "react-jsx",
    "module": "es2020",
    // "rootDir": "./",
     "moduleResolution": "node10",
     "baseUrl": "./src",
     "paths": {
       "*": ["src/*"]
     },
    // "rootDirs": [],
    "typeRoots": ["node_modules/@types"],
    "declaration": true,
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
